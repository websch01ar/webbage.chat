@using System.Web.Optimization

<!DOCTYPE html>
<html ng-app="webbage.app">
<head>
    <title>webbage.chat</title>    

    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script src="//code.angularjs.org/1.2.16/angular.min.js"></script>
    <script src="//code.angularjs.org/1.2.16/angular-route.min.js"></script>
    <script src="//code.angularjs.org/1./angular-cookies.min.js"></script>

    <script src="//cdn.auth0.com/js/lock-6.js"></script>
    <script src="//rawgit.com/auth0/angular-storage/master/dist/angular-storage.js"></script>
    <script src="//rawgit.com/auth0/angular-jwt/master/dist/angular-jwt.js"></script>
    <script src="//cdn.auth0.com/w2/auth0-angular-3.js"></script>

    <meta name="viewport" content="width=device-width, initial-scale=1.0"> 

    @Styles.Render("~/styles/site")
</head>
<body>
    <nav class="navbar navbar-inverse" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">webbage.chat</a>
            </div>
            <div id="right-nav" class="nav navbar-right">
                <img ng-src="{{auth.profile.picture}}" />
                <i ng-show="auth.isAuthenticated" class="glyphicon glyphicon-log-out"></i>
            </div>
        </div>
    </nav>
    <div ng-view></div>

    @Scripts.Render("~/scripts/lib")
    @Scripts.Render("~/scripts/angular")
    
</body>
</html>
